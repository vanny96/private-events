<%assign_current_user%>
<ul>
  <li><%=link_to 'Home', root_path%></li>
  <%if logged_in? %>
    <li><%=link_to 'Profile', @current_user%></li>
    <li><%= link_to 'Logout', logout_path, method: :delete%>
  <%else%>
    <li><%=link_to 'Sign Up', signup_path%></li>
    <li><%=link_to 'Login', login_path%></li>
  <%end%>
</ul>